<ul class="breadcrumb">
	<li>
		<a href="<%= root_path %>">Home</a> <span class="divider">/</span>
	</li>
	<% count = 1 %>
	<% Railgun.interface.breadcrumbs.each do |crumb| %>
	<li<%= " class=active" if count >= Railgun.interface.breadcrumbs.length %>>
	<a href="<%= url_for(crumb[:path]) %>"><%= crumb[:title] %></a>
	<% if count < Railgun.interface.breadcrumbs.length then %><span class="divider">/</span><% end %>
	</li>
	<% count+= 1 %>
	<% end %>
</ul>